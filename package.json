{
  "name": "electron_app",
  "productName": "Electron App",
  "version": "1.0.0",
  "description": "Getting started with Electron",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron App\""
  },
  "author": "Andrew Ng",
  "license": "MIT",
  "devDependencies": {
    "electron": "^9.0.4",
    "electron-packager": "^14.2.1"
  }
}
